    public void swap(int[] nums,int i,int j){
        int temp = nums[i];
        nums[i] = nums[j];
        nums[j] = temp;
    }
    public void sortColors(int[] n) {
        int len = n.length;
        if(len==1)return;
        int ptr1 = 0;
        int ptr2 = len-1;
        for(int i = ptr1; i <= ptr2 ;){
         while(n[i]==2&&ptr2>i)swap(n,i,ptr2--);
         if(n[i]==0)swap(n,i++,ptr1++);
         else i++;
        }
        return;
    }
